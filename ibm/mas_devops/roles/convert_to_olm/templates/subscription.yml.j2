---
apiVersion: operators.coreos.com/v1alpha1
kind: Subscription
metadata:
  name: "ibm-mas{{ mas_app_name_prefix }}-operator"
  namespace: "{{ mas_app_namespace }}"
spec:
  channel: "{{ app_channel }}"
  installPlanApproval: "{{ mas_upgrade_strategy }}"
  name: "ibm-mas{{ mas_app_name_prefix }}"
  source: "{{ app_catalog_source }}"
  sourceNamespace: openshift-marketplace
  startingCSV: "{{ app_starting_csv }}"
