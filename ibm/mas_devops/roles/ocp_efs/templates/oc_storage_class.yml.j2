kind: StorageClass
apiVersion: storage.k8s.io/v1
metadata:
  name: "{{ storage_class_name }}"
provisioner: efs.csi.aws.com
parameters:
  basePath: /rosa
  directoryPerms: '755'
  fileSystemId: "{{ efs_id }}"
  gidRangeEnd: '20000'
  gidRangeStart: '10000'
  provisioningMode: efs-ap
reclaimPolicy: Delete
volumeBindingMode: Immediate
